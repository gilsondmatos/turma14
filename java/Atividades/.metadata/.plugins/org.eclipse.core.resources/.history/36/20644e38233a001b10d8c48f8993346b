package biteBank;

import java.util.Scanner;

public class CriaConta {

	public static void main(String[] args) {
		
		Scanner leia = new Scanner(System.in);

		// Criando mecanismo de referência
		Conta contaGilson = new Conta();// Criou um objeto do tipo conta
		contaGilson.titular = new Cliente();
		
		contaGilson.titular.nome = "Gilson";

		
		contaGilson.deposita(1500);
		System.out.println("Gilson, seu saldo é: " + contaGilson.saldo);
			
	}

}
